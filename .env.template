# N8N Log Exporter Environment Variables
# Copy this file to .env and fill in the values

# PostgreSQL Database Configuration (from your Cloud SQL instance)
DB_POSTGRESDB_HOST=10.75.16.3  # Use your Cloud SQL private IP
DB_POSTGRESDB_PORT=5432
DB_POSTGRESDB_DATABASE=n8n
DB_POSTGRESDB_USER=n8n_user
DB_POSTGRESDB_PASSWORD=your_password_here

# Google Cloud Storage Configuration
GCS_BUCKET_NAME=saas_job_logs
GOOGLE_APPLICATION_CREDENTIALS=/path/to/service-account-key.json

# Optional: Google Cloud Project (if not using default)
GOOGLE_CLOUD_PROJECT=your-project-id

# Logging Configuration (optional)
LOG_LEVEL=INFO

# Example values based on your setup:
# DB_POSTGRESDB_HOST=10.75.16.3
# DB_POSTGRESDB_DATABASE=n8n
# DB_POSTGRESDB_USER=n8n_user
# GCS_BUCKET_NAME=saas_job_logs
